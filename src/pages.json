        "selectedIconPath": "static/icons/shop-activate.png"
      },
      {
        "pagePath": "pages/mine/index",
        "text": "我的",
        "iconPath": "static/icons/mine.png",
        "selectedIconPath": "static/icons/mine-activate.png"
      }
    ]
  },
